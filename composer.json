{
    "name": "cschalenborgh/php-zalando",
    "type": "library",
    "description": "A PHP library for the Zalando API",
    "keywords": ["zalando", "api"],
    "homepage": "https://github.com/cschalenborgh/php-zalando",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Chris Schalenborgh",
            "email": "chris@schalenborgh.be"
        }
    ],
    "support": {
        "issues": "https://github.com/cschalenborgh/php-zalando/issues"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^6.0"
